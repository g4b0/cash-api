{
    "require": {
        "php": ">=7.4",
        "flightphp/core": "^3.18",
        "ext-sqlite3": "*",
        "ext-pdo": "*",
        "ext-json": "*",
        "firebase/php-jwt": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "files": [
            "src/routes.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
